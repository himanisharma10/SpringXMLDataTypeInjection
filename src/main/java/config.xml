<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">
        
    <bean name="himaniStudent" class="com.himani.niet.spring.Student">
    	<property name="studentId" value="123"/>
    	<property name="studentName" value="Himani"/>
    	<property name="studentAddress" value="Greater Nodia" />
    </bean>
    
    <bean name="swatiStudent" class="com.himani.niet.spring.Student">
    	<constructor-arg name="studentId" value="124"/>
    	<constructor-arg name="studentName" value="Swati"/>
    	<constructor-arg name="studentAddress" value="Delhi"/>
    </bean>
    

  	
</beans>